---
layout: null
---
{
  "posts": [
    {% for post in site.categories.projects %}
    {
      "title": "{{ post.project.title | escape }}",
      "location": "{{ post.project.location | escape }}",
      "type": "{{ post.project.type | escape }}",
      "url": "{{ post.project.url | prepend: site.baseurl }}",
      "logo": "{{ post.project.logo | prepend: site.baseurl }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}